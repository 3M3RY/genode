<launcher pkg="solo5" ram="8M">

	<!--
		Uncomment the services required by your unikernel
		in the "config" and "route" sections below.

		To use a binary and block device image from the local
		file-system, see the "solo5_loader" launcher file.

		The size of the heap passed to the unikernel can be
		adjusted using the ram="..." option at the top of this
		file.
	-->

	<config>
		<!-- <solo5 cmdline="Hello Sculpt!"/> -->
		<!-- <rtc/>   -->
		<!-- <nic/>   -->
		<!-- <block/> -->
	</config>

	<route>

	<!-- Load the unikernel binary from "solo5_loader" -->
	<!--
		<service name="ROM" label_last="unikernel.solo5">
			<child name="solo5_loader"/> </service>
	-->

	<!-- RTC not exposed by default -->
	<!--
		<service name="Rtc">
			<parent/> </service>
	-->

	<!-- Networking may be further configured via the "nic_router" -->
	<!--
		<service name="Nic">
			<child name="nic_router"/> </service>
	-->

	<!-- Use a block device provided by "solo5_loader" -->
	<!--
		<service name="Block">
			<child name="solo5_loader"/> </service>
	-->

	</route>

</launcher>

