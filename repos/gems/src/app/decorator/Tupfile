TARGET_NAME = decorator
include_rules
INCLUDES += -I$(REP_DIR)/../demo/include

: $(REP_DIR)/../demo/src/app/scout/data/droidsansb10.tff |> !ln |> %b {font}

: foreach *.rgba {font} |> !incbin |> {obj}
: foreach *.cc |> !cxx |> %B.o {obj}

: {obj} |> !prg |> {bin}
: {bin} |> !collect_bin |>
include &(BIN_RULES)
