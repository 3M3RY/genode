TARGET_NAME = nova_timer_drv
LIBS += genode-base
include_rules

BASE_TIMER_DIR = $(BASE_DIR)/src/timer

INCLUDES += -I.
INCLUDES += -I$(BASE_TIMER_DIR)/include

: foreach *.cc $(BASE_TIMER_DIR)/*.cc |> !cxx |> %B.nova.o {obj}
: {obj} |> !prg |> timer {bin}
: {bin} |> !collect_bin |>

include &(BIN_RULES)
